# Get 2 ints from the user
user_num = int(input())
x = int(input())

# divide the first int by x
user_num = int(user_num / x)
print(user_num, end = ' ')

# divide again
user_num = int(user_num / x)
print(user_num, end = ' ')

# divide again
user_num = int(user_num / x)
print(user_num, end = "\n")
